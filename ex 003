#include <stdio.h>
#include <ctype.h>
#include <cmath>
#include <cstring>

int main() {
    float num1, num2, soma;
    printf("insira um numero: ");
    scanf("%f", &num1);
    printf("insira outro numero: ");
    scanf("%f", &num2);
    soma = num1 + num2;
    printf("a soma eh %f", soma);

    return 0;
}
