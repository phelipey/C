#include <stdio.h>
#include <ctype.h>
#include <cmath>
#include <cstring>

int main() {
    int num1, num2;
    float num3, r1, r2, r3;
    printf("insira um numero inteiro: ");
    scanf("%d", &num1);
    printf("insira outro numero inteiro: ");
    scanf("%d", &num2);
    printf("insira um numero real: ");
    scanf("%f", &num3);
    r1 = (num1 * 2) * (num2 / 2);
    r2 = (num1 * 3) + num3;
    r3 = pow(num3, 3);
    printf("a = %.2f\n", r1);
    printf("b = %.2f\n", r2);
    printf("c = %.2f", r3);
    return 0;
}
